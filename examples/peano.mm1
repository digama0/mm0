theorem isf0: $ isfun 0 $ = '(!! ax_gen x @ !! ax_gen y @ !! ax_gen z @ absurd el02);
theorem app01: $ 0 @ a = 0 $ = '(ndmapp @ mtbir (eleq2 dm0) el02);

theorem disjne (h1: $ G -> A i^i B == 0 $)
  (h2: $ G -> x e. A $) (h3: $ G -> y e. B $):
