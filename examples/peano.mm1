'(axext @ !! ax_gen x @ bitr4gi elb1 (bitr elins @ oreq2 elb0) @
  cases (bithd orl orl) @ oreq2d @ bicomd @ sylbir lt01 bian1);

theorem insdiv2: $ (suc a ; b) // 2 = a ; (b // 2) $ =
