--| `a < b` means `a` is strictly less than `b`.
@(add-eval @ fn (a b) {(eval a) < (eval b)})
@_ def lt (a b: nat): wff = $ suc a <= b $;
infixl lt: $<$ prec 50;

theorem ltletr: $ a < b -> b <= c -> a < c $ = 'letr;
theorem ltletrd (h1: $ G -> a < b $) (h2: $ G -> b <= c $): $ G -> a < c $ = '(letrd h1 h2);

