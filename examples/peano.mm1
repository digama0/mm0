  sbethh (nfim nfv @ nfss nfsbs1 nfsbs1)
    (imeqd (aneq2d eleq1) (sseqd sbsq sbsq)) h));

theorem sabxab {y} (B: set x) (h: $ G -> y e. B -> x e. A $):
  $ G -> S\ x, B == X\ x e. A, B $ =
