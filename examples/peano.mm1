'(axext @ !! ax_gen x @ bitr4gi elins elins @
  bitr4gi (oreq2 elins) (oreq2 elins) or12);

theorem b0ins: $ b0 (a ; b) = suc a ; b0 b $ =
'(axext @ !! ax_gen x @ bitr4gi elb0 (bitr elins @ oreq2 elb0) @
