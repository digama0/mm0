import "compiler.mm1";

do {
  -- Set target to ARM64 macOS
  (mmc-set-target "arm64-macos")

  -- Simple test that just exits with code 42
  (mmc-add '(
    (proc (main : u8)
      42)
  ))
  
  (mmc-finish 'arm64_output)
};

abstract def arm64_output: string = $ arm64_output $;

output string: $ arm64_output $;