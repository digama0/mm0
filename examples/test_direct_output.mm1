import "compiler.mm1";

do {
  (mmc-add '(
    (intrinsic @ proc (sys_write {fd : u32} {count : u32}
      (ghost @ mut {buf : (ref @ array u8 count)}) {p : (&sn buf)} : u32))

    (proc (main {n : u32})
      -- Convert number to simple digit display
      {{digit : u8} := (cast {48 + n})}  -- 48 is ASCII '0'
      {{buf : (array u8 2)} := (list digit 10)}  -- digit + newline
      {_ := (sys_write 1 2 buf (& buf))}
      )
  ))
  
  -- Use mmc->string to bypass proof generation
  (def elf_string (mmc->string))
};

-- Output the ELF file directly
output string: $ elf_string $;