  '(imp @ syl5bi excons @ eexd @ eexda @
    eqtr4d (tokenizeFmlaeq2d @ conseq2d @ syl6eq appendS @ appendeq1d anr) @
    eqtr4d (appendeq1d @ tokenizeFmlaeq2d @ conseq2d anr) @
    casesda (eqtr4d (tokenizeFmlaS_RDelim_lem an3lr anr) @
