theorem decodeTwoUD2_len_le: $ decodeTwoUD2 rex ast b l -> len l <= 6 $ =
'(mpbiri le01 @ leeq1d @ syl6eq len0 @ leneqd anlr);

@_ def decodeTwoSysCall (rex ast b l: nat): wff =
