theorem decodeCMC_T_lem (c: char) (h: $ a e. XAST $):
  $ b = c /\ l = 0 /\ ast = a -> ast e. XAST /\ b e. u8 /\ l e. List u8 $ =
'(iand (iand (anwr @ mpbiri h eleq1) @ mpbiri c2nT @ eleq1d anll) @ mpbiri elList0 @ eleq1d anlr);

@_ def decodeTwoUD2 (rex ast b l: nat): wff =
