@_ def decodeTwoAux (rex ast b l: nat): wff =
$ decodeTwoCMov rex ast b l \/ decodeTwoJCC rex ast b l \/
  decodeTwoSetCC rex ast b l \/ decodeTwoCmpXchg rex ast b l \/
  decodeTwoMovX rex ast b l \/ decodeTwoXadd rex ast b l \/
  decodeTwoUD2 rex ast b l \/ decodeTwoSysCall rex ast b l $;
pub theorem decodeTwoAuxT (rex ast b l: nat):
  $ decodeTwoAux rex ast b l -> ast e. XAST /\ b e. u8 /\ l e. List u8 $ =
(focus
  (def (f x) (atom-app 'decodeTwo x 'T))
  (foldl '(JCC_ SetCC_ CmpXchg MovX_ Xadd UD2 SysCall) (f 'CMov)
    @ fn (x y) '(eor ,x ,(f y))));

theorem decodeTwoAux_len_le: $ decodeTwoAux rex ast b l -> len l <= 6 $ =
(focus
  (def (f x) (atom-app 'decodeTwo x '_len_le))
  (foldl '(JCC SetCC CmpXchg MovX Xadd UD2 SysCall) (f 'CMov)
    @ fn (x y) '(eor ,x ,(f y))));

@_ def decodeTwo (rex ast b l .b2 .l2: nat): wff =
$ b = ch x0 xf /\ E. b2 E. l2 (l = b2 : l2 /\ decodeTwoAux rex ast b2 l2) $;
pub theorem decodeTwoT (rex ast b l: nat):
  $ decodeTwo rex ast b l -> ast e. XAST /\ b e. u8 /\ l e. List u8 $ =
(named '(imp @ eexd @ eexda @ mpand (rsyl anrr decodeTwoAuxT) @
  iand (iand (anwr anll) @ mpbiri c2nT @ eleq1d anll) @
  mpbird (eleq1d @ anld anlr) @ anwr @ sylibr elListS @ anim1 anr));

theorem decodeTwo_len_le: $ decodeTwo rex ast b l -> len l <= 10 $ =
(named '(anwr @ eex @ eex @
  mpbird (leeq1d @ syl6eq lenS @ leneqd anl) @
  sylib lesuc @ anwr @ letrd decodeTwoAux_len_le @ a1i ,norm_num));

@_ def decodeBinopReg (rex ast b l .v .d .opc .reg .r: nat): wff =
$ E. v E. d E. opc E. reg E. r (
    splitBits ((1, v) : (1, d) : (1, 0) : (3, opc) : (2, 0) : 0) b /\
    readModRM rex reg r l /\
    ast = xastBinop opc (opSizeW rex v)
      (if (true d) (R_rm reg r) (Rm_r r reg))) $;
pub theorem decodeBinopRegT (rex ast b l: nat):
  $ decodeBinopReg rex ast b l -> ast e. XAST /\ b e. u8 /\ l e. List u8 $ =
(named @ focus
  '(eex @ eex @ eex @ eex @ eex @ iand (iand
    (mpbird (eleq1d anr) @ sylibr xastBinopT @
      iand (iand _ @ a1i opSizeW_T) @ rsyl anlr @ rsyl readModRM_T @
      sylan ifT (sylibr R_rmT anl) (sylibr Rm_rT @ anwl ancom))
    (anwll @ splitBitsTi ,bvSizeSi))
    (rsyl anlr @ anrd readModRM_T))
  '(syl Binop_low @ sylib elBitvec2 @
    elListHd @ elListTl @ elListTl @ elListTl an3l));

theorem decodeBinopReg_len_le: $ decodeBinopReg rex ast b l -> len l <= 10 $ =
(named '(eex @ eex @ eex @ eex @ eex @ rsyl anlr @ letrd readModRM_len_le @ a1i ,norm_num));

