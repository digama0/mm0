  (mpbird (eleq1d anr) @ sylibr xastCmpXchgT @
    rsyl anlr @ rsyl readModRM_T @ iand (iand (a1i opSizeW_T) anlr) anll)
  (anwll @ splitBitsTi ,bvSizeSi))
  (rsyl anlr @ anrd readModRM_T)));

theorem decodeTwoCmpXchg_len_le: $ decodeTwoCmpXchg rex ast b l -> len l <= 6 $ =
