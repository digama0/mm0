import "compiler.mm1";

do {
  (mmc-add '(
    (intrinsic @ proc (sys_write {fd : u32} {count : u32}
      (ghost @ mut {buf : (ref @ array u8 count)}) {p : (&sn buf)} : u32))

    (proc (main {n : u32})
      -- Use the argument in a computation
      {{result : u32} := {n + 10}}
      
      -- Display result message
      {{msg : (array u8 22)} := (list . ,(string->list "Arg + 10 = 52 (42+10)\n"))}
      {_ := (sys_write 1 22 msg (& msg))}
      
      -- Store result to show computation worked
      {x := result}
      )
  ))
  
  -- Generate and print the ELF
  (print (mmc->string))
};